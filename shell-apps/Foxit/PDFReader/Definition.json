{
    "name": "Foxit PDF Reader",
    "description": "Foxit PDF Reader is a free PDF viewer that allows you to view, print, and annotate PDF documents.",
    "isPublic": true,
    "publisher": "Foxit Software",
    "detectScript": "#detectScript",
    "installScript": "#installScript",
    "uninstallScript": "#uninstallScript",
    "fileUnzip": false,
    "versions": [
        {
            "name": "#version",
            "isPreview": false,
            "installScriptOverride": null,
            "file": {
                "sourceUrl": "#sourceUrl",
                "sha256": "#sha256"
            }
        }
    ],
    "source": {
        "import": true,
        "type": "Evergreen",
        "app": "FoxitReader",
        "filter": "$_.Language -eq \"English\""
    }
}