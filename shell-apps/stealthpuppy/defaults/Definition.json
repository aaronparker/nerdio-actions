{
    "name": "Windows Enterprise Defaults",
    "description": "Make Windows images enterprise ready with optimisations and better defaults.",
    "isPublic": true,
    "publisher": "stealthpuppy",
    "detectScript": "#detectScript",
    "installScript": "#installScript",
    "uninstallScript": "#uninstallScript",
    "fileUnzip": true,
    "versions": [
        {
            "name": "#version",
            "isPreview": false,
            "installScriptOverride": null,
            "file": {
                "sourceUrl": "#sourceUrl",
                "sha256": "#sha256"
            }
        }
    ],
    "source": {
        "type": "Evergreen",
        "app": "WindowsEnterpriseDefaults",
        "filter": "$_.Type -eq \"zip\""
    }
}