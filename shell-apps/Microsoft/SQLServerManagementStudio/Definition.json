                "sourceUrl": "#sourceUrl",
                "sha256": "#sha256"
            }
        }
    ],
    "source": {
        "import": true,
        "type": "Evergreen",
        "app": "MicrosoftSsms",
        "filter": "$_.Language -eq \"English\""
    }
}